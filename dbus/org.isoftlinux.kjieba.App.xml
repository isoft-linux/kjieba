<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.isoftlinux.kjieba.App">
    <method name="query">
      <arg name="term" type="s" direction="in"/>
      <arg name="method" type="x" direction="in"/>
      <arg name="words" type="s" direction="out"/>
    </method>
    <method name="topinyin">
      <arg name="chinese" type="s" direction="in"/>
      <arg name="initial_letter" type="b" direction="in"/>
      <arg name="pinyin" type="s" direction="out"/>
    </method>
  </interface>
</node>
